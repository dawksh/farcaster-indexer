datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Cast {
    id        String   @id @default(cuid())
    fid       BigInt
    timestamp DateTime
    body      Json
    createdAt DateTime @default(now())

    @@unique([fid, timestamp])
}
